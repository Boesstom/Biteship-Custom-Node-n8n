{
  "name": "n8n-nodes-biteship",
  "version": "1.0.0",
  "description": "n8n custom node untuk integrasi dengan Biteship API",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "biteship",
    "shipping",
    "courier"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Boesstom/n8n-nodes-biteship",
  "author": {
    "name": "Boesstom",
    "email": "boesstom@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boesstom/n8n-nodes-biteship.git"
  },
  "main": "index.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BiteshipApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Biteship/Biteship.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "~1.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "~1.2.0"
  }
}
